# jenkinsfile

pipeline{
  
  agent {
    label 'java'
  }
  
  stages('build'){
    
    steps{
      println "From declarative pipeline script - build stage"
    }
  
  }
  
  stage('deploy'){
    
    steps{
      println "From declarative pipeline script - deploy stage"
    }
    
  }
  
}

}

  
